---
  ecmaFeatures:
    modules: true
    classes: true
    blockBindings: true
    arrowFunctions: true
    spread: true
    restParams: true
    defaultParams: false
    destructuring: false
    forOf: true
    generators: false

    templateStrings: false

    binaryLiterals: false
    octalLiterals: false
    unicodeCodePointEscapes: false
    regexUFlag: false
    regexYFlag: false


    superInFunctions: false
    objectLiteralComputedProperties: false
    objectLiteralShorthandMethods: false
    objectLiteralShorthandProperties: false

    objectLiteralDuplicateProperties: false
    globalReturn: false

  rules:
    semi: never
    quotes: single avoid-escape
    no-use-before-define: nofunc
    curly: 2 multi-line
    no-underscore-dangle: 0
    no-comma-dangle: 0
    new-parens: 0
    no-proto: 0
    global-strict: 0
    no-constant-condition: 1
    no-spaced-func: 1
    key-spacing: 1 align colon
    new-cap:
      - 2
      - capIsNew: false
